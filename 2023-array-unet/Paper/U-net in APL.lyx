\end_layout

\begin_layout Verbatim
¯14
\end_layout

\begin_layout Verbatim
      2 × (3 - (4 × 5))
\end_layout

\begin_layout Verbatim
¯34
\end_layout

\begin_layout Verbatim
      2 × 3 - 4 × 5
\end_layout

\begin_layout Verbatim
¯34
\end_layout

\begin_layout Standard
APL uses the high-minus 
\family typewriter
¯
\family default
